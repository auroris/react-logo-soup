{
  "name": "react-logo-soup",
  "version": "0.1.0",
  "description": "A tiny React library that makes logos look good together",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target browser --format esm --sourcemap=external --external react --external react-dom && bun build src/index.ts --outfile dist/index.cjs --target browser --format cjs --sourcemap=inline --external react --external react-dom && bun run build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sanity-labs/react-logo-soup.git"
  },
  "homepage": "https://github.com/sanity-labs/react-logo-soup#readme",
  "bugs": {
    "url": "https://github.com/sanity-labs/react-logo-soup/issues"
  },
  "keywords": [
    "react",
    "logo",
    "normalize",
    "layout",
    "component"
  ],
  "license": "MIT",
  "devDependencies": {
    "@chromatic-com/storybook": "^3",
    "@storybook/addon-essentials": "^8",
    "@storybook/addon-interactions": "^8",
    "@storybook/addon-links": "^8",
    "@storybook/blocks": "^8",
    "@storybook/react": "^8",
    "@storybook/react-vite": "^8",
    "@storybook/test": "^8",
    "@happy-dom/global-registrator": "^16",
    "@testing-library/react": "^16",
    "@types/bun": "latest",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "react": "^18",
    "react-dom": "^18",
    "storybook": "^8",
    "vite": "^5"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18",
    "typescript": "^5"
  }
}
