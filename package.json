{
  "name": "logo-soup",
  "version": "0.2.0",
  "description": "A tiny library that makes logos look good together",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bun build src/global.ts --outfile dist/logo-soup.min.js --target browser --minify && bun build src/index.ts --outdir dist --target browser --format esm --sourcemap=external && bun run build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "test": "bun test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sanity-labs/react-logo-soup.git"
  },
  "homepage": "https://github.com/sanity-labs/react-logo-soup#readme",
  "bugs": {
    "url": "https://github.com/sanity-labs/react-logo-soup/issues"
  },
  "keywords": [
    "logo",
    "normalize",
    "layout",
    "vanilla-js"
  ],
  "license": "MIT",
  "devDependencies": {
    "@happy-dom/global-registrator": "^16",
    "@types/bun": "latest"
  }
}
